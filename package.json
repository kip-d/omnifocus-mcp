{
  "name": "omnifocus-mcp-cached",
  "version": "1.14.1",
  "description": "Professional MCP server for OmniFocus with smart caching and analytics",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "omnifocus-mcp-cached": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node ./dist/index.js",
    "test": "vitest tests/unit",
    "test:integration": "node tests/integration/test-as-claude-desktop.js",
    "test:e2e": "node tests/e2e/claude-desktop-protocol.test.ts",
    "test:all": "npm run test && npm run test:integration",
    "lint": "eslint src --ext .ts",
    "lint:strict": "eslint src --ext .ts --config .eslintrc.mcp.json",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "omnifocus",
    "productivity",
    "task-management"
  ],
  "author": "Guillaume",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@types/node": "^24.0.3",
    "typescript": "^5.8.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "chai": "^5.2.0",
    "eslint": "^9.29.0",
    "globals": "^16.3.0",
    "vitest": "^3.2.4"
  }
}
