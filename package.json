{
  "name": "omnifocus-mcp-cached",
  "version": "2.0.0",
  "description": "MCP server for OmniFocus v2 - Production release with 95% performance improvements",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "omnifocus-mcp-cached": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node ./dist/index.js",
    "test": "vitest tests/unit",
    "test:integration": "node tests/integration/test-as-claude-desktop.js",
    "test:performance": "vitest tests/performance --run",
    "test:coverage": "vitest tests/unit --coverage --run",
    "test:all": "npm run test && npm run test:integration",
    "test:quick": "vitest tests/unit --run --reporter=verbose",
    "test:quick:safe": "VITEST_SAFE=1 vitest tests/unit --run --reporter=verbose --exclude tests/unit/integration.test.ts --exclude tests/unit/test-data-management.test.ts",
    "cleanup:test-data": "node scripts/cleanup-test-data.js",
    "lint": "eslint src --ext .ts",
    "lint:strict": "eslint src --ext .ts --config .eslintrc.mcp.json",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "analyze-failures": "npx tsx scripts/analyze-tool-failures.ts",
    "ci:local": "./scripts/local-ci.sh"
  },
  "keywords": [
    "mcp",
    "omnifocus",
    "productivity",
    "task-management"
  ],
  "author": "Kip DeGraaf <kip@2kip.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@types/node": "^24.0.3",
    "typescript": "^5.8.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "chai": "^5.2.0",
    "eslint": "^9.29.0",
    "globals": "^16.3.0",
    "tsx": "^4.20.4",
    "vitest": "^3.2.4",
    "zod-to-json-schema": "^3.23.3"
  }
}
